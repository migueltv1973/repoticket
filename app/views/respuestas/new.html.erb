<h1>New Respuesta</h1>

<%= form_for @ticket_Thread do |f| %>
<div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
</div>

<div class="field">
    <%= f.label :descripcion %><br>
    <%= f.text_area :descripcion %>
</div>

<div class="field">
    <%= f.label :ticket_id %><br>
    <%= f.text_field :ticket_id %>
</div>

<%= f.submit "Save changes", class: "btn btn-primary" %>
<% end %>

<%= link_to 'Back', respuestas_path %>