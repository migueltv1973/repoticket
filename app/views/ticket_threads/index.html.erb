<p id="notice"><%= notice %></p>

<h1>Respuesta Ticket</h1>

<table>
<thead>
<tr>
  <th>Title</th>
  <th>descripcion</th>
  <th>correo</th>
  <th>Acciones</th>
  <th colspan="3"></th>
</tr>
</thead>

  <tbody>
    <% @ticket_threads.each do |ticket_thread| %>

      <tr>
      <td><%= ticket_thread.title %></td>
      <td><%= ticket_thread.descripcion %></td>
      <td><%= ticket_thread.email %></td>
      
        <td><%= link_to 'Show', ticket_thread %></td>
        <td><%= link_to 'Edit', edit_ticket_thread_path(ticket_thread) %></td>
        <td><%= link_to 'Destroy', ticket_thread, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket Respuesta', new_ticket_thread_path %>
<%= link_to 'Back', tickets_path %>
